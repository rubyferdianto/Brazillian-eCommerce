# Environment Template File
# Copy this file to .env and update with your actual credentials
# DO NOT commit .env file to git!

# MongoDB Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourApp
MONGO_DATABASE=brazilian-ecommerce

# Google Cloud Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCS_BUCKET=your-gcs-bucket-name
GCP_REGION=us-central1

# Dataflow Configuration
DATAFLOW_MACHINE_TYPE=n1-standard-2
DATAFLOW_MAX_WORKERS=10
